<%= form_for [@skill, @fundi] do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :surname %>
    <%= f.text_field :surname %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.label :contacts %>
    <%= f.text_field :contacts %>
    <%= f.label :about %>
    <%= f.text_area :about %>
    <p><%= f.submit %></p>
  <% end %>
